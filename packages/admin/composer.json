{
    "name": "quocdaijr/admin",
    "description": "Fork from mdmsoft/yii2-admin, RBAC Auth manager for Yii2 with view use Bootstrap4",
    "keywords": ["yii", "admin", "auth", "rbac"],
    "type": "package",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "quocdaijr",
            "email": "quocdaijr@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "yiisoft/yii2": "~2.0.7"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "~2.0"
    },
    "suggest": {
        "yiisoft/yii2-bootstrap4": "~2.0.6",
        "npm-asset/font-awesome": "^5.15"
    },
    "autoload": {
        "psr-4": {
            "admin\\": ""
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
